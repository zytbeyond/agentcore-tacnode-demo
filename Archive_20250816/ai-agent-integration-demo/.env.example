# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here

# AWS Bedrock AgentCore Configuration
BEDROCK_AGENT_ID=your_agent_id_here
BEDROCK_AGENT_ALIAS_ID=your_agent_alias_id_here
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_REGION=us-east-1

# Strands Agents SDK Configuration
STRANDS_API_KEY=your_strands_api_key_here
STRANDS_WORKSPACE_ID=your_workspace_id_here
STRANDS_MODEL_PROVIDER=bedrock
STRANDS_LOG_LEVEL=INFO

# Tacnode Database Configuration
TACNODE_HOST=localhost
TACNODE_PORT=5432
TACNODE_DATABASE=ai_agents_demo
TACNODE_USERNAME=tacnode_user
TACNODE_PASSWORD=your_tacnode_password_here
TACNODE_SSL_MODE=prefer
TACNODE_CONNECTION_POOL_SIZE=20
TACNODE_MAX_OVERFLOW=30

# Vector Database Configuration
VECTOR_DIMENSION=1536
VECTOR_INDEX_TYPE=ivf_flat
VECTOR_SIMILARITY_METRIC=cosine
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Graph Database Configuration (Neo4j for comparison)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# Performance Testing Configuration
PERFORMANCE_TEST_DURATION=300
PERFORMANCE_CONCURRENT_USERS=50
PERFORMANCE_RAMP_UP_TIME=30
PERFORMANCE_METRICS_INTERVAL=5

# Demo Configuration
DEMO_MODE=development
DEMO_LOG_LEVEL=DEBUG
DEMO_ENABLE_METRICS=true
DEMO_ENABLE_TRACING=true
DEMO_SAMPLE_DATA_SIZE=1000

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_RELOAD=true

# Monitoring Configuration
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
JAEGER_PORT=16686

# Development Configuration
DEBUG=true
TESTING=false
LOG_FORMAT=json
LOG_FILE=logs/demo.log
